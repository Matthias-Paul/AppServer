{
  "name": "mediaunboxed",
  "version": "1.0.0",
  "description": "An Electron application with React",
  "main": "./out/main/index.js",
  "author": "mediaunboxed.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "prebuild": "cd backend && npm install",

    "clean": "rm -rf dist/ out/ node_modules/.cache/ ~/.cache/electron ~/.cache/electron-builder",
    "clean:build": "npm run clean && npm run build:mac",
    "prebuild:mac": "rm -rf dist/ build/ out/",
    "clean:builder": "npx electron-builder install-app-deps --force",


    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps && cd backend && npm install",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@tailwindcss/vite": "^4.1.8",
    "@tanstack/react-query": "^5.80.6",
    "qrcode": "^1.5.4",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-qr-code": "^2.0.15",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.2",
    "react-select": "^5.10.1",
    "recharts": "^2.15.3",
    "redux-persist": "^6.0.0",
    "sequelize-cli": "^6.6.3",
    "tailwindcss": "^4.1.8",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "nodemon": "^3.1.10",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "sudo-prompt": "^9.2.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.0.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^35.1.5",
    "electron-builder": "^26.0.12",
    "electron-vite": "^3.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "vite": "^6.2.6"
  },
  "build": {
    "appId": "com.mediaunboxed.desktop",
    "productName": "MediaUnboxed",
    "files": [
      "out/**/*",
      "backend/**/*",
      ".env",
      "!backend/node_module_old/**/*",
      "!backend/node_modules/.cache/**/*"
    ],
    "extraResources": [
      {
        "from": ".env",
        "to": ".env",
        "filter": ["**/*"]
      },
      {
        "from": "backend/.env",
        "to": "backend/.env",
        "filter": ["**/*"]
      }
    ],
    "directories": {
      "output": "dist"
    },
    "mac": {
      "icon": "build/icon.icns",
      "category": "public.app-category.productivity",
      "target": [
        {
          "target": "pkg",
          "arch": ["x64", "arm64"]
        },
        {
          "target": "dmg",
          "arch": ["x64", "arm64"]
        }
      ],
      "identity": "W7B8LSD8Q4"
    },
    "win": {
      "icon": "build/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": ["x64"]
        }
      ]
    },
    "linux": {
      "icon": "build/icon.png",
      "target": ["AppImage", "deb"]
    }
  }
}
